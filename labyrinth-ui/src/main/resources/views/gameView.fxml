<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="me.sunstorm.labyrinth.ui.controller.GameController"
           prefHeight="600"
           prefWidth="960"
           stylesheets="/styles-compiled/gameView.css"
           styleClass="blurred-gradient">
    <VBox alignment="CENTER">
        <GridPane fx:id="gridPane" maxHeight="500" maxWidth="500" styleClass="padding-20">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="100.0"
                styleClass="basic-button" text="%navigation.back" fx:id="menuButton" onMouseClicked="#menuClicked"/>
    </VBox>
</StackPane>
